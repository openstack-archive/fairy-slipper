{
  "profile": {
    "spec": {
      "version": "1.0", 
      "type": "os.heat.stack", 
      "properties": {
        "files": {
          "file:///usr/test_script.sh": "#!/bin/bash\n\necho \"this is a test script file\"\n"
        }, 
        "disable_rollback": false, 
        "template": {
          "outputs": {
            "result": {
              "value": {
                "get_attr": [
                  "random", 
                  "value"
                ]
              }
            }
          }, 
          "heat_template_version": "2014-10-16", 
          "resources": {
            "random": {
              "type": "OS::Heat::RandomString", 
              "properties": {
                "length": 64
              }
            }
          }, 
          "parameters": {
            "file": {
              "default": {
                "get_file": "file:///usr/test_script.sh"
              }, 
              "type": "string"
            }
          }
        }, 
        "parameters": {}, 
        "environment": {
          "resource_registry": {
            "os.heat.server": "OS::Heat::Server"
          }
        }, 
        "timeout": 60
      }
    }, 
    "name": "test_prof1", 
    "metadata": {}
  }
}